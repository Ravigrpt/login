<h1>Sign Up</h1>
<%= form_for @user do |f| %>
 
	<p>
		<%= f.label :username %>
		<%= f.text_field :username %>
	</p>

	<p>
		<%= f.label :password %>
		<%= f.text_field :password %>
	</p>

	<p>
		<%= f.label :conformpassword %>
		<%= f.text_field :conformpassword %>
	</p>

	<%= f.submit  "create users" %>

<% end %>